height = 176
weight = 64
height_m = height / 100
bmi = weight / (height_m * height_m)
print "BMIは", bmi
if bmi < 18.5 { print "低体重" }
if bmi >= 18.5 and bmi < 25.0 { print "普通体重" }
if bmi >= 25.0 and bmi < 30.0 { print "肥満（１度）" }
if bmi >= 30.0 and bmi < 35.0 { print "肥満（２度）" }
if bmi >= 35.0 and bmi < 40.0 { print "肥満（３度）" }
if bmi >= 40.0 { print "肥満（４度）" }
